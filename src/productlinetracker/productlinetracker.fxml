<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.cell.*?>
<?import javafx.collections.*?>
<GridPane styleClass="tab-header-background" alignment="center" hgap="10" prefHeight="470.0"
  prefWidth="650.0" vgap="10"
  xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="productlinetracker.Controller">
  <columnConstraints>
    <ColumnConstraints/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints/>
  </rowConstraints>
  <TabPane styleClass="tab" prefHeight="470.0" prefWidth="657.0" tabClosingPolicy="UNAVAILABLE">
    <Tab styleClass="tab-label" text="Product Line">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <GridPane layoutX="128.0" layoutY="8.0" prefHeight="141.0" prefWidth="323.0">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          </rowConstraints>
          <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <Label styleClass="label" layoutX="51.0" layoutY="17.0"
              text="Product Name:"/>
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
            <TextField fx:id="prodNameTA" layoutX="14.0" layoutY="13.0" prefHeight="30.0"
              prefWidth="117.0"/>
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="170.0" GridPane.rowIndex="1">
            <Label styleClass="label" layoutX="57.0" layoutY="16.0"
              text="Manufacturer:"/>
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
            GridPane.rowIndex="1">
            <TextField fx:id="manufacturerTA" layoutX="14.0" layoutY="13.0"
              prefHeight="30.0" prefWidth="117.0"/>
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
            <Label styleClass="label" layoutX="81.0" layoutY="16.0" text="Item Type:"/>
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1"
            GridPane.rowIndex="2">
            <ChoiceBox fx:id="itemTypeChoiceBox" layoutX="14.0" layoutY="11.0"
              prefHeight="21.0" prefWidth="115.0">
            </ChoiceBox>
          </AnchorPane>
        </GridPane>
        <Button styleClass="button" layoutX="206.0" layoutY="159.0" mnemonicParsing="false"
          onAction="#handleEventAddProduct" prefHeight="31.0" prefWidth="213.0"
          text="Add Product"/>
        <Label layoutX="10.0" layoutY="190.0" text="Existing Products"/>
        <TableView styleClass="tableView" layoutX="10.0" layoutY="211.0" prefHeight="200.0"
          prefWidth="629.0">
          <columns>
            <TableColumn text="Product Name" prefWidth="210">
              <cellValueFactory>
                <PropertyValueFactory property="name"/>
              </cellValueFactory>
            </TableColumn>
            <TableColumn text="Manufacturer" prefWidth="210">
              <cellValueFactory>
                <PropertyValueFactory property="manufacturer"/>
              </cellValueFactory>
            </TableColumn>
            <TableColumn text="Item Type" prefWidth="210">
              <cellValueFactory>
                <PropertyValueFactory property="type"/>
              </cellValueFactory>
            </TableColumn>
          </columns>
          <items>
            <FXCollections fx:factory="observableArrayList">
            </FXCollections>
          </items>

        </TableView>
      </AnchorPane>
    </Tab>
    <Tab styleClass="tab-label" text="Produce">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <Label layoutX="271.0" layoutY="14.0" text="Choose Product:"/>
        <ListView layoutX="225.0" layoutY="35.0" prefHeight="200.0" prefWidth="200.0"/>
        <Label layoutX="268.0" layoutY="255.0" text="Choose Quantity:"/>
        <ComboBox fx:id="productAmtComboBox" layoutX="250.0" layoutY="276.0"
          prefWidth="150.0"/>
        <Button layoutX="253.0" layoutY="340.0" mnemonicParsing="false"
          onAction="#handleEventRecordProduction" text="Record Production"/>
      </AnchorPane>
    </Tab>
    <Tab styleClass="tab-label" text="Production Log">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <TextArea layoutX="40.0" layoutY="41.0" prefHeight="354.0" prefWidth="568.0"/>
      </AnchorPane>
    </Tab>
  </TabPane>
</GridPane>
