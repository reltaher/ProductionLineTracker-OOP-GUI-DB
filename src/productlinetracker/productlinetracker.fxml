<GridPane alignment="center" hgap="10" prefHeight="470.0" prefWidth="650.0" styleClass="tab-header-background" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="productlinetracker.Controller">
  <columnConstraints>
    <ColumnConstraints />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
  </rowConstraints>
  <TabPane prefHeight="470.0" prefWidth="657.0" styleClass="tab" tabClosingPolicy="UNAVAILABLE">
    <Tab styleClass="product-tab" text="Product Line">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <GridPane layoutX="128.0" layoutY="8.0" prefHeight="141.0" prefWidth="323.0">
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <Label layoutX="51.0" layoutY="17.0" styleClass="label" text="Product Name:" />
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
            <TextField fx:id="prodNameTA" layoutX="14.0" layoutY="13.0" prefHeight="30.0" prefWidth="117.0" />
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="170.0" GridPane.rowIndex="1">
            <Label layoutX="57.0" layoutY="16.0" styleClass="label" text="Manufacturer:" />
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <TextField fx:id="manufacturerTA" layoutX="14.0" layoutY="13.0" prefHeight="30.0" prefWidth="117.0" />
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
            <Label layoutX="81.0" layoutY="16.0" styleClass="label" text="Item Type:" />
          </AnchorPane>
          <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <ChoiceBox fx:id="itemTypeChoiceBox" layoutX="14.0" layoutY="11.0" prefHeight="21.0" prefWidth="115.0">
            </ChoiceBox>
          </AnchorPane>
        </GridPane>
        <Button layoutX="206.0" layoutY="159.0" mnemonicParsing="false" onAction="#handleEventAddProduct" prefHeight="31.0" prefWidth="213.0" styleClass="button" text="Add Product" />
        <Label layoutX="10.0" layoutY="190.0" text="Existing Products" />
        <TableView fx:id="tableView" layoutX="10.0" layoutY="211.0" prefHeight="200.0" prefWidth="629.0" styleClass="tableView">
          <columns>
            <TableColumn fx:id="productNameColumn" prefWidth="210" text="Product Name">
              <cellValueFactory>
                <PropertyValueFactory property="name" />
              </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="manufacturerColumn" prefWidth="210" text="Manufacturer">
              <cellValueFactory>
                <PropertyValueFactory property="manufacturer" />
              </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="itemTypeColumn" prefWidth="210" text="Item Type">
              <cellValueFactory>
                <PropertyValueFactory property="type" />
              </cellValueFactory>
            </TableColumn>
          </columns>
          <items>
            <FXCollections fx:factory="observableArrayList">
            </FXCollections>
          </items>

        </TableView>
      </AnchorPane>
    </Tab>
    <Tab styleClass="product-tab" text="Produce">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <Label layoutX="271.0" layoutY="14.0" text="Choose Product:" />
        <ListView fx:id="listView" layoutX="225.0" layoutY="35.0" prefHeight="200.0" prefWidth="200.0" />
        <Label layoutX="268.0" layoutY="255.0" text="Choose Quantity:" />
        <ComboBox fx:id="productAmtComboBox" layoutX="250.0" layoutY="276.0" prefWidth="150.0" />
        <Button layoutX="253.0" layoutY="340.0" mnemonicParsing="false" onAction="#handleEventRecordProduction" text="Record Production" />
      </AnchorPane>
    </Tab>
    <Tab styleClass="product-tab" text="Production Log">
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <TextArea layoutX="40.0" layoutY="41.0" prefHeight="354.0" prefWidth="568.0" />
      </AnchorPane>
    </Tab>
  </TabPane>
</GridPane>
